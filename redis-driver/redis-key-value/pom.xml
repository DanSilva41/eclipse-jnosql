<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
 <modelVersion>4.0.0</modelVersion>
 <artifactId>redis-key-value</artifactId>
 <packaging>pom</packaging>
 <name>Redis key-value driver to Apache diana Project</name>
 <parent>
  <groupId>org.apache.diana.redis.driver</groupId>
  <artifactId>redis-driver-parent</artifactId>
  <version>0.0.1-SNAPSHOT</version>
</parent>
<properties>
  <maven.compile.targetLevel>1.8</maven.compile.targetLevel>
  <maven.compile.sourceLevel>1.8</maven.compile.sourceLevel>
</properties>
<dependencies>
  <dependency>
    <groupId>redis.clients</groupId>
    <artifactId>jedis</artifactId>
    <version>2.7.2</version>
  </dependency>
  <dependency>
    <groupId>com.google.code.gson</groupId>
    <artifactId>gson</artifactId>
    <version>2.7</version>
  </dependency>
</dependencies>
<build>
  <plugins>
    <plugin>
      <groupId>org.apache.maven.plugins</groupId>
      <artifactId>maven-compiler-plugin</artifactId>
      <version>${maven.compile.version}</version>
      <configuration>
        <target>1.8</target>
        <source>1.8</source>
      </configuration>
    </plugin>
  </plugins>
</build>
<modules>
  <module>redis-key-value-api</module>
  <module>redis-key-value-tck</module>
</modules>
</project>
